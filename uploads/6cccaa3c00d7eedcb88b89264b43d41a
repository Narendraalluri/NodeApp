function FindProxyForURL(url, host)
{
if (isPlainHostName(host))
	return "DIRECT";

if (dnsDomainIs(host, ".alltel.com"))
	return "PROXY  alltel.proxy.vzwcorp.com:5150";

if (dnsDomainIs(host, "csr-portal.msn.com"))
	return "PROXY  fhdcproxy.verizon.com:80";

if (shExpMatch(host, "cloud.verizon.com") ||
	shExpMatch(host, "clouddemo.verizon.com") ||
	shExpMatch(host, "apps.verizon.com"))
	return "PROXY fhdcproxy.verizon.com:80;"  +
	       "PROXY fdcproxy.verizon.com:80";

